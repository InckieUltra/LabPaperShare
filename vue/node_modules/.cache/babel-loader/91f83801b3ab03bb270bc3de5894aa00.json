/el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"tableData\"\r\n        border\r\n        stripe\r\n        style=\"width: 1100px;margin-top: 10px;\">\r\n      <el-table-column\r\n          prop=\"title\"\r\n          label=\"论文标题\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"authors\"\r\n          label=\"论文作者\">\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"conference\"\r\n          label=\"发布会议\">\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"date\"\r\n          label=\"发布时间\">\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" >\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" type=\"primary\" @click=\"seeDetail(scope.row)\">详情</el-button>\r\n          <el-button size=\"mini\" @click=\"seeFile(scope.row)\">附件</el-button>\r\n          <el-button size=\"mini\" @click=\"deletePaper(scope.row)\" >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[1,3,5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport request from \"@/utils/request\";\r\nimport comment from \"@/components/Comment\";\r\nexport default {\r\n  name: 'Role',\r\n  components: {\r\n    comment\r\n  },\r\n  data() {\r\n    return {\r\n      drawer:false,\r\n      loading: true,\r\n      field_id:'',\r\n      paper_id:'',\r\n      field_name:'',\r\n      form: {},\r\n      dialogVisible: false,\r\n      bookVis: false,\r\n      search: '',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      totalPage:1,\r\n      tableData: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.field_id = this.$route.query.field_id\r\n    this.field_name = this.$route.query.field_name\r\n    this.load()\r\n\r\n    //this.setCurrentPageData()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.loading = true\r\n      console.log(\"okNow\")\r\n      let page_no = this.currentPage-1\r\n      request.post(\"/api/findpaperbyfield?field_id=\"+this.field_id+\"&page_no=\"+page_no +\"&page_size=\"+this.pageSize).then(res => {\r\n        if (res.code === 0){\r\n          this.tableData = res.data[1]\r\n          this.total = res.data[0]\r\n          console.log(res)\r\n        }\r\n      })\r\n      this.loading = false\r\n\r\n    },\r\n\r\n    seeDetail(row) {\r\n      console.log(\"seeDetail: \")\r\n      console.log(row)\r\n\r\n      this.$router.push({path: '/detail',query:{paper_id:row.paper_id }})\r\n\r\n    },\r\n    seeFile(row) {\r\n      console.log(row)\r\n    },\r\n    deletePaper(row) {\r\n      console.log(\"seeComment: \")\r\n      console.log(row)\r\n      //this.paper_id = row.paper_id\r\n      this.drawer=true;\r\n    },\r\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}