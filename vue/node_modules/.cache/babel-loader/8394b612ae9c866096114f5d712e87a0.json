    <el-table-column\n          prop=\"date\"\n          label=\"发布时间\">\n      </el-table-column>\n      <el-table-column label=\"操作\" >\n        <template #default=\"scope\">\n          <el-button size=\"mini\" type=\"primary\" @click=\"seeDetail(scope.row)\">详情</el-button>\n          <el-button size=\"mini\" @click=\"seeFile(scope.row)\">附件</el-button>\n          <el-button size=\"mini\" @click=\"deletePaper(scope.row)\" >删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"currentPage\"\n          :page-sizes=\"[1,3,5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n  </div>\n</template>\n\n<script>\n\n\nimport request from \"@/utils/request\";\nimport comment from \"@/components/Comment\";\nexport default {\n  name: 'Role',\n  components: {\n    comment\n  },\n  data() {\n    return {\n      drawer:false,\n      loading: true,\n      field_id:'',\n      paper_id:'',\n      field_name:'',\n      form: {},\n      dialogVisible: false,\n      bookVis: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      totalPage:1,\n      tableData: [],\n    }\n  },\n  created() {\n    this.field_id = this.$route.query.field_id\n    this.field_name = this.$route.query.field_name\n    this.load()\n\n    //this.setCurrentPageData()\n  },\n  methods: {\n    load() {\n      this.loading = true\n      console.log(\"okNow\")\n      let page_no = this.currentPage-1\n      request.post(\"/api/findpaperbyfield?field_id=\"+this.field_id+\"&page_no=\"+page_no +\"&page_size=\"+this.pageSize).then(res => {\n        if (res.code === 0){\n          this.tableData = res.data[1]\n          this.total = res.data[0]\n          console.log(res)\n        }\n      })\n      this.loading = false\n\n    },\n\n    seeDetail(row) {\n      console.log(\"seeDetail: \")\n      console.log(row)\n\n      this.$router.push({path: '/detail',query:{paper_id:row.paper_id }})\n\n    },\n    seeFile(row) {\n      console.log(row)\n    },\n    deletePaper(row) {\n      console.log(\"seeComment: \")\n      console.log(row)\n      //this.paper_id = row.paper_id\n      this.drawer=true;\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.currentPage = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}