    </el-dropdown-menu>\n        </template>\n      </el-dropdown>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {Sunrise} from \"@element-plus/icons\";\n\nexport default {\n  name: \"Header\",\n  props: ['user'],\n  data() {\n    return {\n\n    }\n  },\n  created() {\n  },\n  components: {\n    Sunrise,\n\n  },\n  methods:{\n    loginOut() {\n      sessionStorage.removeItem(\"user\")\n      this.$router.push('/login')\n    }\n  }\n}\n</script>\n\n<style>\n.item {\n  margin-top: 10px;\n  margin-right: 40px;\n}\n</style>\n"],"sourceRoot":""}]}