AA,WAAhE,C,qCAhBuD,E,MAiBd,I,CAAK,U,CAAW,M,KAAM,C,kBAA/D,mBAAA,CAIM,KAJN,EAAA,WAAA,EAIM,C,WAAA,G,kBAHJ,mBAAA,CAE2B,SAF3B,EAE2B,IAF3B,EAE2B,WAAA,CAFJ,KAAA,CAAA,IAAA,CAAK,UAED,EAFW,UAApB,CAAoB,EAAnB;iCAAnB,YAAA,CAE2B,oBAF3B,EAE2B;AAFa,cAAA,IAAI,EAAC,MAElB;AAF0B,cAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,uBAAE,QAAA,CAAA,OAAA,CAAQ,CAAC,CAAC,QAAV,CAAF;AAAA;AAE/B,aAF3B,E;gCAEI;AAAA,uBAAW,C,kCAAT,CAAC,CAAC,K,GAAK;AAAA;iBAAE,CAAX;AAAA,e;;;;aAFJ,E;;AAAA,c,WAAA,C;WAE2B,CAF3B,E;;AAAA,WAGI,EAJN,C,qCAjBuD,EAuBvD,YAAA,CAKa,qBALb,EAKa;AALD,gCAAiB;AAKhB,WALb,EAAqC;8BACnC;AAAA,qBAGM,CAHN,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFa,KAAA,CAAA,IAAA,KAAI,CAAJ,IAAc,KAAA,CAAA,OAAA,KAAO,KAAA,CAAS,IAAT,CAAc,O,kBAApD,YAAA,CAA8G,oBAA9G,EAA8G;sBAAA;AAAhD,gBAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,yBAAE,QAAA,CAAA,eAAA,CAAgB,KAAA,CAAA,IAAhB,CAAF;AAAA,iBAAA;AAA2C,eAA9G,E;kCAA4F;AAAA,yBAAM,C,WAAA,CAAN;AAAA,iB;;;;eAA5F,C,qCAEI,E,MADkB,I,CAAK,K,CAAM,M,KAAM,C,kBAAvC,YAAA,CAA4F,oBAA5F,EAA4F;sBAAA;AAA9C,gBAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,yBAAE,QAAA,CAAA,SAAA,CAAU,KAAA,CAAA,IAAA,CAAK,KAAf,CAAF;AAAA,iBAAA;AAAyC,eAA5F,E;kCAA4E;AAAA,yBAAI,C,WAAA,CAAJ;AAAA,iB;;;;eAA5E,C,qCACI,CAHN,CAGM,CAHN;AAAA,a,CADmC;;;;AAAA,WAArC,CAvBuD,EA8BvD,YAAA,CACU,kBADV,EACU;AADD,YAAA,KAAmB,EAAnB;AAAA,uBAAA;AAAA,aACC;AADoB,YAAA,OAAO,EAAA,KAAA,CAAO;AAClC,WADV,E,IAAA,E;;AAAA,Y,WAAA,CA9BuD,CAAvD;AAAA,S,CAH+B;;;;AAAA,OAAjC,CADY,CAHZ;AAAA,K,CAyCa;;;;AAAA,GA1Cf,CA4CI,CA7CN,C","sourcesContent":["<template>\r\n  <div>\r\n    <el-container>\r\n      <el-header>\r\n        <h1 style=\"text-align: center\">{{form.title}}</h1>\r\n        <div style=\"text-align: center\">{{form.authors}}</div>\r\n      </el-header>\r\n      <el-main style=\"margin-top: 10px\">\r\n\r\n\r\n        <div style=\"font-size: 15px;\">摘要：{{form.summary}}</div>\r\n        <el-divider></el-divider>\r\n        <div style=\"font-size: 15px;\">发布会议：{{form.conference}}</div>\r\n        <el-divider></el-divider>\r\n        <div style=\"font-size: 15px;\">发布日期：{{form.date}}</div>\r\n        <el-divider></el-divider>\r\n        <div style=\"font-size: 15px;\">所属领域：{{form.field}}</div>\r\n        <el-divider></el-divider>\r\n        <div style=\"font-size: 15px;\">论文链接：\r\n          <el-button @click='routeClick(form.link)' type=\"text\">{{form.link}}</el-button>\r\n          </div>\r\n        <el-divider></el-divider>\r\n        <div style=\"font-size: 15px;\">笔记内容：\r\n        <div v-html=\"this.form.content\"></div>\r\n        </div>\r\n\r\n        <el-divider v-if=\"this.form.references.length!==0\"></el-divider>\r\n        <div style=\"font-size: 10px;\" v-if=\"this.form.references.length!==0\">引用文献：\r\n          <el-button v-for=\"a in form.references\" type=\"text\" @click=\"jumpRef(a.paper_id)\">\r\n\r\n              {{a.title}}</el-button>\r\n        </div>\r\n\r\n        <el-divider content-position=\"center\">\r\n          <div style=\"margin:0px auto\">\r\n            <el-button v-if=\"role === 1 || user_id ===this.form.user_id\" @click=\"jumpPaperChange(form)\">修改论文信息</el-button>\r\n            <el-button v-if=\"this.form.files.length!==0\" @click=\"downFiles(form.files)\">下载附件</el-button>\r\n          </div>\r\n        </el-divider>\r\n\r\n        <comment style=\"width: 100%\" :paperid =this.paper_id>\r\n        </comment>\r\n\r\n      </el-main>\r\n    </el-container>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport comment from \"@/components/Comment\";\r\nimport request from \"@/utils/request\";\r\nexport default {\r\n  name: \"Detail\",\r\n  inject:['reload'],\r\n  components:{\r\n    comment\r\n  },\r\n  data(){\r\n    return{\r\n      field_name:'',\r\n      paper_id:'',\r\n      user_id:'',\r\n      role:'',\r\n      lastPath:'',\r\n      form:{\r\n        field:String,\r\n        files:[],\r\n        references:[]\r\n      },\r\n      Req:{\r\n        serverfile:'',\r\n        savefile:''\r\n      }\r\n    }\r\n  },\r\n\r\n  methods:{\r\n    jumpRef(paperid){\r\n      window.location.href='detail?paper_id='+paperid\r\n      //this.$router.push({path: '/detail',query:{paper_id: paperid }})\r\n    },\r\n    routeClick(e){\r\n      window.location.href = e;\r\n    },\r\n    jumpPaperChange(row){\r\n      this.$router.push({path:'/changePaper', query:{origin_paper_id: row.paper_id,upload_id : row.upload_id}})\r\n    },\r\n    downFiles(files){\r\n      for (let i = 0;i<files.length;i++){\r\n        this.Req.serverfile = files[i]\r\n        this.Req.savefile = '/Users/liujiaming/Downloads//'+files[i]\r\n        //console.log(this.Req)\r\n        request.post(\"/api/file/download\",this.Req).then(res => {\r\n          if (res.code === 0){\r\n            console.log(res)\r\n            let url = window.URL.createObjectURL(new Blob([res.data])) // 将获取的文件转化为blob格式\r\n            let a = document.createElement('a'); // 此处向下是打开一个储存位置\r\n            a.style.display = 'none';\r\n            a.href = url;\r\n            // 下面两行是自己项目需要的处理，总之就是得到下载的文件名（加后缀）即可\r\n            //var fileNameArray = path.split('#')[1].split('/')\r\n            //var fileName = fileNameArray[fileNameArray.length-1]\r\n\r\n            a.setAttribute('download',files[i]);\r\n            document.body.appendChild(a);\r\n            a.click();//点击下载\r\n            document.body.removeChild(a);// 下载完成移除元素\r\n            window.URL.revokeObjectURL(url);// 释放掉blob对象\r\n            this.$message.success(\"下载成功\")\r\n            //console.log(res.msg)\r\n          }else{\r\n            this.$message.error(\"下载失败\")\r\n            //console.log(res.msg)\r\n          }\r\n        })\r\n      }\r\n\r\n    }\r\n  },\r\n  created() {\r\n    let userStrr = sessionStorage.getItem(\"user\")\r\n    this.role = JSON.parse(userStrr).role\r\n    this.user_id = JSON.parse(userStrr).user_id\r\n    this.paper_id = this.$route.query.paper_id\r\n    console.log(\"jiazai详情\")\r\n    request.post(\"/api/paper?paper_id=\"+this.paper_id).then(res => {\r\n      if (res.code === 0){\r\n        this.form = res.data\r\n        console.log(this.form)\r\n        var temp\r\n        for (var i = 0;i<res.data.fields.length;i++){\r\n          //console.log(res.data.fields[i].field_name)\r\n          if (i===0){\r\n            temp = res.data.fields[i].field_name\r\n          }else{\r\n            temp= temp + ','+ res.data.fields[i].field_name\r\n          }\r\n        }\r\n        this.form.field=temp\r\n        for (var i = 0;i<res.data.authors.length;i++){\r\n          //console.log(res.data.fields[i].field_name)\r\n          if (i===0){\r\n            temp = res.data.authors[i]\r\n          }else{\r\n            temp= temp + ','+ res.data.authors[i]\r\n          }\r\n        }\r\n        this.form.authors=temp\r\n      }else{\r\n        this.$message.error(\"加载失败\")\r\n        //console.log(res.msg)\r\n      }\r\n    })\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}